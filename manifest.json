  "update_url": "http://www.moreslowly.jp/chrome/extensions/dicbutton/updates.xml",
  "content_scripts": [ {
      "js": [ "dicbutton.js" ],
      "matches": [ "http://www.google.com/search*",
                   "http://www.google.co.jp/search*" ],
      "run_at": "document_start"
  } ]
}
